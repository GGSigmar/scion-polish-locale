void gf_ButtonUpgrades () {
    // Automatic Variable Declarations
    // Implementation
    if ((gv_shiftToggle == false) && (gv_allAbilities == 0)) {
        gf_AllAbilitiesOn();
    }
    else if ((gv_shiftToggle == false) && (gv_allAbilities == 1)) {
        gf_AllAbilitiesOff();
